package projetoF;

import static org.junit.Assert.*;

import org.junit.Test;

public class testeIdade {

	@Test
	public void testidade() {
		
		
		int n1 =5;
		int n2 = 4;
		diferenciaDeIdade di = new diferenciaDeIdade();
