package projetoA;

public class produto {

	
	private double peso;
	private double altura;
	
	private double imc;
	
	public double produto(double peso, double altura) {
		
		imc = peso+altura;
		return imc;
		
	}
	
	
	public double getPeso() {
		return peso;
	}
	public void setPeso(double peso) {
		this.peso = peso;
	}
	public double getAltura() {
		return altura;
	}
	public void setAltura(double altura) {
		this.altura = altura;
	}
	
}
